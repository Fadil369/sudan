# Vite Environment Variables
# Variables prefixed with VITE_ are exposed to client-side code

# API Configuration
VITE_API_BASE_URL=https://api.sudan.elfadil.com
VITE_API_VERSION=v1

# OID Configuration
VITE_OID_ROOT=1.3.6.1.4.1.61026

# App Metadata
VITE_APP_NAME=Sudan Digital Government Portal
VITE_APP_VERSION=1.0.0

# Feature Flags
VITE_ENABLE_BIOMETRIC=true
VITE_ENABLE_OFFLINE=true
VITE_ENABLE_PWA=true

# Analytics (optional)
VITE_ANALYTICS_ID=

