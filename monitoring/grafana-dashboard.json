{
  "dashboard": {
    "id": null,
    "title": "ðŸ‡¸ðŸ‡© Sudan Digital Identity System - Main Dashboard",
    "tags": ["sudan", "identity", "government", "monitoring"],
    "timezone": "UTC+03:00",
    "refresh": "30s",
    "schemaVersion": 30,
    "version": 1,
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"]
    },
    "panels": [
      {
        "id": 1,
        "title": "Sudan Identity System Overview",
        "type": "stat",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "up{job=\"sudan-identity-portal\"}",
            "legendFormat": "Portal Status"
          },
          {
            "expr": "sum(rate(http_requests_total{job=\"sudan-identity-portal\"}[5m]))",
            "legendFormat": "Requests/sec"
          },
          {
            "expr": "sum(up{job=~\".*-ministry\"})",
            "legendFormat": "Ministry Services Online"
          },
          {
            "expr": "up{job=\"postgresql\"}",
            "legendFormat": "Database Status"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short",
            "min": 0
          },
          "overrides": [
            {
              "matcher": {"id": "byName", "options": "Portal Status"},
              "properties": [
                {"id": "color", "value": {"mode": "thresholds"}},
                {"id": "thresholds", "value": {
                  "steps": [
                    {"color": "red", "value": 0},
                    {"color": "green", "value": 1}
                  ]
                }}
              ]
            }
          ]
        },
        "options": {
          "orientation": "horizontal",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "textMode": "value_and_name"
        }
      },
      {
        "id": 2,
        "title": "Citizen Portal Traffic",
        "type": "timeseries",
        "gridPos": {"h": 9, "w": 12, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{job=\"sudan-identity-portal\"}[5m]))",
            "legendFormat": "Total Requests/sec"
          },
          {
            "expr": "sum(rate(http_requests_total{job=\"sudan-identity-portal\",status=~\"2..\"}[5m]))",
            "legendFormat": "Success Requests/sec"
          },
          {
            "expr": "sum(rate(http_requests_total{job=\"sudan-identity-portal\",status=~\"4..\"}[5m]))",
            "legendFormat": "Client Errors/sec"
          },
          {
            "expr": "sum(rate(http_requests_total{job=\"sudan-identity-portal\",status=~\"5..\"}[5m]))",
            "legendFormat": "Server Errors/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "reqps",
            "min": 0
          }
        },
        "options": {
          "tooltip": {"mode": "multi", "sort": "none"},
          "legend": {"displayMode": "visible", "placement": "bottom"}
        }
      },
      {
        "id": 3,
        "title": "Response Times",
        "type": "timeseries",
        "gridPos": {"h": 9, "w": 12, "x": 12, "y": 8},
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket{job=\"sudan-identity-portal\"}[5m]))",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, rate(http_request_duration_seconds_bucket{job=\"sudan-identity-portal\"}[5m]))",
            "legendFormat": "95th percentile"
          },
          {
            "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket{job=\"sudan-identity-portal\"}[5m]))",
            "legendFormat": "99th percentile"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "s",
            "min": 0
          }
        },
        "options": {
          "tooltip": {"mode": "multi", "sort": "desc"},
          "legend": {"displayMode": "visible", "placement": "bottom"}
        }
      },
      {
        "id": 4,
        "title": "Ministry Services Status",
        "type": "stat",
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 17},
        "targets": [
          {
            "expr": "up{job=\"health-ministry\"}",
            "legendFormat": "Health Ministry"
          },
          {
            "expr": "up{job=\"education-ministry\"}",
            "legendFormat": "Education Ministry"
          },
          {
            "expr": "up{job=\"interior-ministry\"}",
            "legendFormat": "Interior Ministry"
          },
          {
            "expr": "up{job=\"finance-ministry\"}",
            "legendFormat": "Finance Ministry"
          },
          {
            "expr": "up{job=\"energy-ministry\"}",
            "legendFormat": "Energy Ministry"
          },
          {
            "expr": "up{job=\"infrastructure-ministry\"}",
            "legendFormat": "Infrastructure Ministry"
          },
          {
            "expr": "up{job=\"justice-ministry\"}",
            "legendFormat": "Justice Ministry"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            },
            "unit": "short",
            "mappings": [
              {"options": {"0": {"text": "DOWN"}}, "type": "value"},
              {"options": {"1": {"text": "UP"}}, "type": "value"}
            ]
          }
        },
        "options": {
          "orientation": "horizontal",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "textMode": "name"
        }
      },
      {
        "id": 5,
        "title": "Database Performance",
        "type": "timeseries",
        "gridPos": {"h": 9, "w": 8, "x": 0, "y": 25},
        "targets": [
          {
            "expr": "pg_stat_activity_count",
            "legendFormat": "Active Connections"
          },
          {
            "expr": "rate(pg_stat_database_tup_inserted[5m])",
            "legendFormat": "Inserts/sec"
          },
          {
            "expr": "rate(pg_stat_database_tup_updated[5m])",
            "legendFormat": "Updates/sec"
          },
          {
            "expr": "rate(pg_stat_database_tup_deleted[5m])",
            "legendFormat": "Deletes/sec"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short",
            "min": 0
          }
        }
      },
      {
        "id": 6,
        "title": "Blockchain Network Health",
        "type": "timeseries",
        "gridPos": {"h": 9, "w": 8, "x": 8, "y": 25},
        "targets": [
          {
            "expr": "sum(up{job=\"hyperledger-fabric\"})",
            "legendFormat": "Active Peers"
          },
          {
            "expr": "rate(fabric_transactions_total[5m])",
            "legendFormat": "Transactions/sec"
          },
          {
            "expr": "fabric_block_height",
            "legendFormat": "Block Height"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short",
            "min": 0
          }
        }
      },
      {
        "id": 7,
        "title": "Biometric Processing",
        "type": "timeseries", 
        "gridPos": {"h": 9, "w": 8, "x": 16, "y": 25},
        "targets": [
          {
            "expr": "rate(biometric_verifications_total[5m])",
            "legendFormat": "Verifications/sec"
          },
          {
            "expr": "biometric_matching_accuracy",
            "legendFormat": "Matching Accuracy %"
          },
          {
            "expr": "histogram_quantile(0.95, rate(biometric_processing_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile processing time"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short",
            "min": 0
          }
        }
      },
      {
        "id": 8,
        "title": "System Resources",
        "type": "timeseries",
        "gridPos": {"h": 9, "w": 12, "x": 0, "y": 34},
        "targets": [
          {
            "expr": "100 - (avg(irate(node_cpu_seconds_total{mode=\"idle\"}[5m])) * 100)",
            "legendFormat": "CPU Usage %"
          },
          {
            "expr": "(1 - (node_memory_MemAvailable_bytes / node_memory_MemTotal_bytes)) * 100",
            "legendFormat": "Memory Usage %"
          },
          {
            "expr": "(1 - (node_filesystem_avail_bytes{mountpoint=\"/\"} / node_filesystem_size_bytes{mountpoint=\"/\"})) * 100",
            "legendFormat": "Disk Usage %"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "steps": [
                {"color": "green", "value": 0},
                {"color": "yellow", "value": 70},
                {"color": "red", "value": 90}
              ]
            }
          }
        }
      },
      {
        "id": 9,
        "title": "Website Uptime Status",
        "type": "stat",
        "gridPos": {"h": 9, "w": 12, "x": 12, "y": 34},
        "targets": [
          {
            "expr": "probe_success{instance=\"https://sudan-identity.gov.sd\"}",
            "legendFormat": "Main Portal"
          },
          {
            "expr": "probe_success{instance=\"https://api.sudan.gov.sd/health\"}",
            "legendFormat": "API Gateway"
          },
          {
            "expr": "avg(probe_success{instance=~\"https://.*-api.sudan.gov.sd.*\"})",
            "legendFormat": "Ministry APIs"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "thresholds"},
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "yellow", "value": 0.8},
                {"color": "green", "value": 0.95}
              ]
            },
            "unit": "percentunit",
            "mappings": [
              {"options": {"0": {"text": "DOWN"}}, "type": "value"},
              {"options": {"1": {"text": "UP"}}, "type": "value"}
            ]
          }
        },
        "options": {
          "orientation": "horizontal",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "textMode": "value_and_name"
        }
      },
      {
        "id": 10,
        "title": "Chat Support Metrics",
        "type": "timeseries",
        "gridPos": {"h": 9, "w": 8, "x": 0, "y": 43},
        "targets": [
          {
            "expr": "rate(chat_sessions_total[5m])",
            "legendFormat": "New Sessions/sec"
          },
          {
            "expr": "chat_active_sessions",
            "legendFormat": "Active Sessions"
          },
          {
            "expr": "histogram_quantile(0.95, rate(chat_response_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile response time"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short",
            "min": 0
          }
        }
      },
      {
        "id": 11,
        "title": "Error Analysis",
        "type": "piechart",
        "gridPos": {"h": 9, "w": 8, "x": 8, "y": 43},
        "targets": [
          {
            "expr": "sum(rate(http_requests_total{job=\"sudan-identity-portal\",status=~\"4..\"}[5m]))",
            "legendFormat": "4xx Client Errors"
          },
          {
            "expr": "sum(rate(http_requests_total{job=\"sudan-identity-portal\",status=~\"5..\"}[5m]))",
            "legendFormat": "5xx Server Errors"
          },
          {
            "expr": "sum(rate(http_requests_total{job=\"sudan-identity-portal\",status=~\"2..\"}[5m]))",
            "legendFormat": "2xx Success"
          }
        ],
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          },
          "pieType": "donut"
        }
      },
      {
        "id": 12,
        "title": "Security Events",
        "type": "logs",
        "gridPos": {"h": 9, "w": 8, "x": 16, "y": 43},
        "targets": [
          {
            "expr": "rate(http_requests_total{status=\"401\"}[5m])",
            "legendFormat": "Authentication Failures"
          },
          {
            "expr": "rate(http_requests_total{status=\"403\"}[5m])",
            "legendFormat": "Authorization Failures"
          },
          {
            "expr": "rate(suspicious_activity_total[5m])",
            "legendFormat": "Suspicious Activities"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {"mode": "palette-classic"},
            "unit": "short",
            "min": 0
          }
        }
      }
    ],
    "annotations": {
      "list": [
        {
          "name": "Deployments",
          "datasource": "prometheus",
          "enable": true,
          "expr": "changes(sudan_identity_version[1d])",
          "iconColor": "blue",
          "step": "1m",
          "tagKeys": "version",
          "textFormat": "Deployment: {{version}}",
          "titleFormat": "New Deployment"
        }
      ]
    },
    "templating": {
      "list": [
        {
          "name": "ministry",
          "type": "query",
          "query": "label_values(up{job=~\".*-ministry\"}, job)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "multi": true
        },
        {
          "name": "instance", 
          "type": "query",
          "query": "label_values(up{job=\"sudan-identity-portal\"}, instance)",
          "refresh": 1,
          "includeAll": true,
          "allValue": ".*",
          "multi": true
        }
      ]
    },
    "links": [
      {
        "title": "Sudan Identity System Documentation",
        "url": "https://docs.sudan.gov.sd/identity-system",
        "type": "link"
      },
      {
        "title": "Alert Manager",
        "url": "https://alerts.sudan.gov.sd",
        "type": "link"
      },
      {
        "title": "System Health Dashboard",
        "url": "/d/system-health/sudan-system-health",
        "type": "dashboards"
      }
    ]
  },
  "overwrite": true,
  "message": "Sudan Digital Identity System - Main Monitoring Dashboard"
}